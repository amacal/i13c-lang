asm exit(code@rdi: u8[0x00..0x0f]) noreturn clobbers rcx, r11, rax {
    mov rax, 0x3c;
    syscall;
}

asm shift(count@imm: u8[0x00..0x3f]) clobbers rdi {
    shl rdi, count;
}

fn x(count: u8[0x00..0x1f]) {
  shift(0x01);
}

fn y(count: u8[0x20..0x3f]) {
  shift(0x02);
}

fn main() noreturn {
    x(0x10);
    y(0x30);
    exit(0x01);
}
