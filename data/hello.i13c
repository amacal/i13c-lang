asm exit(code@rdi: u8[0x00..0x0f]) noreturn clobbers rcx, r11, rax {
    mov rax, 0x3c;
    syscall;
}

asm shift(count@imm: u8[0x00..0x3f]) clobbers rdi {
    shl rdi, count;
}

fn x(count: u8[0x00..0x1f]) {
  shift(0x01);
}

fn y(count: u8[0x20..0x3f]) {
  shift(0x02);
}

fn foo(val1: u8[0x00..0x1f], val2: u8[0x20..0x3f], val3: u8[0x00..0x0f]) noreturn {
  x(val1); y(val2); exit(val3);
}

fn main() noreturn {
    foo(0x010, 0x30, 0x02);
}
