asm exit(code@rdi: u32[0x01..0x0f], left@imm: u16[0x01..0x02]) noreturn {
    shl rdi, left;
    mov rax, 0x3c;
    syscall;
}

asm shift(left@imm: u16[0x01..0x02]) {
    shl rdi, left;
}

fn f1() {
    shift(0x01);
}

fn f2() {
    shift(0x02);
}

fn aux() noreturn {
    f2();
    exit(0x01, 0x02);
}

fn main() noreturn {
    f1();
    aux();
}
